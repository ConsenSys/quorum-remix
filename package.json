{
  "name": "quorum-remix",
  "version": "0.1.4",
  "private": true,
  "homepage": "https://remix-plugin.goquorum.com/",
  "dependencies": {
    "@remixproject/plugin": "^0.1.11",
    "axios": "^0.19.0",
    "copy-to-clipboard": "^3.2.0",
    "dotenv-flow": "^3.1.0",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "^3.3.0",
    "react-select": "^3.0.8",
